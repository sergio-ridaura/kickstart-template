{
  "name": "kickstart-template",
  "title": "Kickstart template",
  "version": "1.4.0",
  "description": "Kickstart template for my website developments.",
  "lang": "en",
  "keywords": [
    "react",
    "mysql",
    "bash",
    "cli",
    "node",
    "typescript",
    "modules",
    "astro",
    "assemblyscript",
    "tailwind"
  ],
  "author": {
    "name": "Sergio Ridaura",
    "url": "https://github.com/sergio-ridaura",
    "email": "sergio.ridaura@outlook.com"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rm -R dist .astro .vercel;",
    "dev": "astro dev --host",
    "build": "npm run clean; astro telemetry disable; astro check; export $(cat .env.runtime) && export $(cat .env.prod) && astro build;",
    "preview": "SERVER_KEY_PATH=./private/key.pem SERVER_CERT_PATH=./private/cert.pem HOST=0.0.0.0 node ./dist/server/entry.mjs",
    "start": "npm run preview",
    "update": "npm i @astrojs/check @astrojs/node @astrojs/sitemap @astrojs/tailwind @astrojs/vercel astro dotenv tailwindcss typescript"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/node": "^8.2.5",
    "@astrojs/sitemap": "^3.1.5",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.6.0",
    "astro": "^4.9.2",
    "dotenv": "^16.4.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  }
}
