{
  "name": "kickstart-template",
  "title": "Kickstart template",
  "version": "1.2.0",
  "description": "Kickstart template for my website developments.",
  "lang": "en",
  "keywords": [
    "react",
    "mysql",
    "bash",
    "cli",
    "node",
    "typescript",
    "modules",
    "astro",
    "assemblyscript",
    "tailwind"
  ],
  "author": {
    "name": "Sergio Ridaura",
    "url": "https://github.com/sergio-ridaura",
    "email": "sergio.ridaura@outlook.com"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rm -R dist .astro;",
    "dev": "astro dev --host",
    "build": "npm run clean; astro check; astro build;",
    "start": "HOST=0.0.0.0 PORT=80; astro preview --host --port 80",
    "astro": "astro",
    "update": "npm i @astrojs/check @astrojs/tailwind astro tailwindcss typescript"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/tailwind": "^5.1.0",
    "astro": "^4.9.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  }
}
